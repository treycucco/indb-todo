{
  "name": "indb-todo",
  "type": "module",
  "homepage": "https://treycucco.github.io/indb-todo",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "build": "tsc && vite build",
    "check": "npm run test:typecheck && npm run test:prettier && npm run test:eslint",
    "deploy": "gh-pages -d dist",
    "dev": "vite",
    "predeploy": "npm run build",
    "preview": "vite preview",
    "test:eslint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "test:prettier": "prettier --check src/",
    "test:typecheck": "tsc"
  },
  "dependencies": {
    "@indb/database": "^0.2.0",
    "@indb/preact": "^0.2.0",
    "preact": "^10.16.0"
  },
  "devDependencies": {
    "@preact/preset-vite": "^2.5.0",
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "gh-pages": "^6.0.0",
    "prettier": "^3.0.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  }
}
